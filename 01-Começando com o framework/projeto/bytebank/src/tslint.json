{
  "extends": ["tslint:recommended", "tslint-plugin-prettier", "tslint-config-prettier"]
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ]
  }
}
